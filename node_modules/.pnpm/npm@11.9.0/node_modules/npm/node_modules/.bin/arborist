#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/myapi/node_modules/.pnpm/npm@11.9.0/node_modules/npm/node_modules/@npmcli/arborist/bin/node_modules:/workspaces/myapi/node_modules/.pnpm/npm@11.9.0/node_modules/npm/node_modules/@npmcli/arborist/node_modules:/workspaces/myapi/node_modules/.pnpm/npm@11.9.0/node_modules/npm/node_modules/@npmcli/node_modules:/workspaces/myapi/node_modules/.pnpm/npm@11.9.0/node_modules/npm/node_modules:/workspaces/myapi/node_modules/.pnpm/npm@11.9.0/node_modules:/workspaces/myapi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/myapi/node_modules/.pnpm/npm@11.9.0/node_modules/npm/node_modules/@npmcli/arborist/bin/node_modules:/workspaces/myapi/node_modules/.pnpm/npm@11.9.0/node_modules/npm/node_modules/@npmcli/arborist/node_modules:/workspaces/myapi/node_modules/.pnpm/npm@11.9.0/node_modules/npm/node_modules/@npmcli/node_modules:/workspaces/myapi/node_modules/.pnpm/npm@11.9.0/node_modules/npm/node_modules:/workspaces/myapi/node_modules/.pnpm/npm@11.9.0/node_modules:/workspaces/myapi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@npmcli/arborist/bin/index.js" "$@"
else
  exec node  "$basedir/../@npmcli/arborist/bin/index.js" "$@"
fi
